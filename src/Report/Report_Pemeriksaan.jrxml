	<queryString language="SQL">
		<![CDATA[SELECT
    p.id_pemeriksaan AS `Id Pemeriksaan`,
    CONCAT('[', ps.id_pasien, '] ', ps.nama_pasien) AS `No Urut / Nama Pasien`,
    p.keluhan AS `Keluhan`,
    p.diagnosa AS `Diagnosa`,
    p.tindakan AS `Tindakan`,
    jp.tanggal AS `Tanggal`
FROM pemeriksaan p
JOIN jadwal_pemeriksaan jp 
    ON p.id_jadwal = jp.id_jadwal
JOIN pasien ps 
    ON jp.id_pasien = ps.id_pasien
ORDER BY jp.tanggal ASC;]]>
	</queryString>
