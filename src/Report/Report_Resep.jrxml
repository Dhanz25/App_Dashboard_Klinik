	<queryString language="SQL">
		<![CDATA[SELECT
    r.id_resep AS `Id Resep`,
    ps.nama_pasien AS `Nama Pasien`,
    p.keluhan AS `Keluhan`,
    o.nama_obat AS `Nama Obat`,
    ro.jumlah AS `Jumlah`,
    ro.aturan_pakai AS `Aturan Pakai`
FROM resep_obat r
JOIN resep_obat ro 
    ON r.id_resep = ro.id_resep
JOIN obat o 
    ON ro.id_obat = o.id_obat
JOIN pemeriksaan p 
    ON r.id_pemeriksaan = p.id_pemeriksaan
JOIN jadwal_pemeriksaan jp 
    ON p.id_jadwal = jp.id_jadwal
JOIN pasien ps 
    ON jp.id_pasien = ps.id_pasien
ORDER BY r.id_resep ASC;]]>
	</queryString>
