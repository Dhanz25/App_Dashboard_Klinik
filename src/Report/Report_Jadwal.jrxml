	<queryString language="SQL">
		<![CDATA[SELECT 
    j.id_jadwal       AS "ID Jadwal",
    j.no_urut         AS "No Urut",
    p.nama_pasien     AS "Nama Pasien",
    DATE(j.tanggal)   AS "Tanggal",
    d.nama_dokter     AS "Nama Dokter"
FROM jadwal_pemeriksaan j
JOIN pasien p 
    ON j.id_pasien = p.id_pasien
JOIN dokter d 
    ON j.id_dokter = d.id_dokter
ORDER BY 
    DATE(j.tanggal) ASC,
    j.no_urut ASC;]]>
	</queryString>
