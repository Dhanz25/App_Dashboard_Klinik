	<queryString language="SQL">
		<![CDATA[SELECT 
    lp.id_pengguna,
    lp.username,
    lp.level,
    CASE 
        WHEN lp.level = '2' THEN d.nama_dokter
        ELSE lp.username
    END AS nama_dokter
FROM login_pengguna lp
LEFT JOIN dokter d 
    ON lp.id_dokter = d.id_dokter;]]>
	</queryString>
