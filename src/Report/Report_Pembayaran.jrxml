	<queryString language="SQL">
		<![CDATA[SELECT jp.id_jadwal, jp.tanggal, ps.nama_pasien, d.nama_dokter, p.diagnosa, p.tindakan, o.nama_obat, ro.jumlah, o.harga, IFNULL(pb.total_bayar, 0) AS total_bayar, IFNULL(pb.status, 'BELUM LUNAS') AS status FROM pemeriksaan p JOIN jadwal_pemeriksaan jp ON jp.id_jadwal = p.id_jadwal JOIN resep_obat ro ON ro.id_pemeriksaan = p.id_pemeriksaan JOIN obat o ON o.id_obat = ro.id_obat LEFT JOIN pembayaran pb ON pb.id_pemeriksaan = p.id_pemeriksaan JOIN pasien ps ON ps.id_pasien = jp.id_pasien JOIN dokter d ON d.id_dokter = jp.id_dokter;]]>
